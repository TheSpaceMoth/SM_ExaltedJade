<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="RockBase">
    <defName>MineableBlueJade</defName>
    <label>Blue Jade</label>
    <description>Rock containing bits of blue jade.</description>
    <graphicData>
      <texPath>Things/Building/Linked/RockFlecked_Atlas</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <color>(0,36,85)</color>
      <colorTwo>(44,96,165)</colorTwo>
    </graphicData>
    <statBases>
      <MaxHitPoints>1500</MaxHitPoints>
    </statBases>
    <building>
      <isResourceRock>true</isResourceRock>
      <mineableThing>BlueJade</mineableThing>
      <mineableYield>40</mineableYield>
      <mineableScatterCommonality>0.065</mineableScatterCommonality>
      <mineableScatterLumpSizeRange>2~8</mineableScatterLumpSizeRange>
    </building>
  </ThingDef>
  
